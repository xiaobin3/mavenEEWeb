<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>

    <filter>
        <filter-name>GenericEncodingFilter</filter-name>
        <filter-class>com.xiaobin.web.filter.GenericEncodingFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>GenericEncodingFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>

    <servlet>
        <servlet-name>AddCustomerServlet</servlet-name>
        <servlet-class>com.xiaobin.web.AddCustomerServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddCustomerServlet</servlet-name>
        <url-pattern>/addCustomerServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>AddCustServlet</servlet-name>
        <servlet-class>com.xiaobin.web.AddCustServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddCustServlet</servlet-name>
        <url-pattern>/addCustServlet</url-pattern>
    </servlet-mapping>
</web-app>
